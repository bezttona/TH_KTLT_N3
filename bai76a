#include <stdio.h>
#include <conio.h>

void nhapMaTran(int A[100][100], int n) {
    printf("Nhập các phần tử của ma trận %dx%d:\n", n, n);
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("A[%d][%d] = ", i+1, j+1);
            scanf("%d", &A[i][j]);
        }
    }
}
void xuatMaTran(int A[100][100], int n) {
    printf("Ma trận A là:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }
}

int main() {
    int n;
    int A[100][100];

    printf("Nhập cấp của ma trận n (n x n): ");
    scanf("%d", &n);
    

    
    nhapMaTran(A, n);

    
    xuatMaTran(A, n);

    getch();
}
